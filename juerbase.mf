%% Ligature table
if not known has_minuscule:
	boolean has_minuscule;
	has_minuscule := false;
fi;

if has_minuscule:
  % when lower case has different glyph set then upper case
  ligtable "A":
  	"L" kern -1.2u#, "M" kern -.6u#,
  	"l" kern -1.2u#, "m" kern -.6u#;
  ligtable "E":
  	"M" kern -.6u#, "N" kern -1.2u#,
  	"m" kern -.6u#, "n" kern -1.2u#, skipto 1;
  ligtable "F":
  	"E" kern -2u#,
  	"e" kern -2u#;
  ligtable "T":
  	"E" kern -4u#, "A" kern -2u#, "M" kern -2u#, 
  	"e" kern -4u#, "a" kern -2u#, "m" kern -2u#, skipto 1;
  ligtable "O":"P":"Q":"R":
  1::
    "H" kern -.6u#,
    "I" kern -.6u#,
    "L" kern -.6u#,
    "O" kern -.6u#,
    "Q" kern -.6u#,
    "R" kern -.6u#,
    "S" kern -.6u#,
    "U" kern -.6u#,
    "V" kern -0.6u#,
    "h" kern -.6u#,
    "i" kern -.6u#,
    "l" kern -.6u#,
    "o" kern -.6u#,
    "q" kern -.6u#,
    "r" kern -.6u#,
    "s" kern -.6u#,
    "u" kern -.6u#,
    "v" kern -0.6u#;
  ligtable "M":
