turningcheck := 0;
def beginjur(expr ch, width) =
	beginchar(ch, width, 10pt#, 0pt#) enddef;

num_width := 7u#;
aheight := .75;

beginjur(" ", 5u#);
relax;
endchar;

beginjur("0", num_width);
	penmonglyph(mg_t) ;
	bot z1 = (0.5w, 0);
	rt z2 = (w - sx#/2, 0.5hh#);
	top z3 = (0.5w, hh#);
	lft z4 = (sx#, 0.5hh#);
	%draw z1..z2..z3..z4..cycle;
	penpos1(mg_t, -30);
	penpos2(mg_t, 30);
	penpos3(mg_t, 120);
	penpos4(mg_t, 170);
	penstroke z1e..z2e..z3e..z4e..cycle;
	penlabels(1,2,3,4);
endchar;

beginjur("1", num_width);
	penmonglyph(mg_t) ;
	top lft z1 = (sx#/2, hh#);
	top rt z2 = (w-sx#, hh#);

	penpos1(mg_t, 20);
	penpos2(mg_t, 20);
	penstroke z1e--z2e;

	rt z22 = (w-sx#, hh#);
	bot rt z3 = (w-sx#, 0);
	penpos22(mg_t, 20);
	penpos3(mg_t, 15);
	penstroke z22e--z3e;


	z5 = ((x1+x2)/2, y1);
	z6 = (xpart z5, 0.5 hh#);
	penpos5(mg_t, 20);
	penpos6(mg_t, 10);
	penstroke z5e--z6e;

	% bottom horizontal
	rt bot z8 = (w-sx#, 0);
	penpos8(mg_t, 40);
	bot z4 = (0.5w, 0);
	penpos4(mg_t, 40);
	penstroke z4e--z8e;

	penlabels(1,2,3,4,22,5,6,8);
endchar;

