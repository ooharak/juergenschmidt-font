	y6 = (y2+y8)/2;
	z4 = (.7(x5+x6), y6);
	y5 = y4;
	x5 = sx#;
	x7 = (x5 + x4) / 2;
	y7 = y8;
	x3 = x7;
	x1 = (x3 + x4) / 2;
