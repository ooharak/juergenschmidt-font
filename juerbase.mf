turningcheck := 0;
def beginjur(expr ch, width) =
	beginchar(ch, width, 10pt#, 0pt#) enddef;

num_width := 7u#;
aheight := .75;

% lower cases
lx := .6;
ly := .6;


beginjur(" ", 5u#);
relax;
endchar;

% BEGIN_NUMERIC 
%
beginjur("1", num_width);
	penmonglyph(mg_t) ;
	top lft z1 = (sx#/2, hh#);
	top rt z2 = (w-sx#, hh#);

	penpos1(mg_t, 20);
	penpos2(mg_t, 20);
	penstroke z1e--z2e;

	rt z22 = (w-sx#, hh#);
	bot rt z3 = (w-sx#, 0);
	penpos22(mg_t, 20);
	penpos3(mg_t, 15);
	penstroke z22e--z3e;


	z5 = ((x1+x2)/2, y1);
	z6 = (xpart z5, 0.5 hh#);
	penpos5(mg_t, 20);
	penpos6(mg_t, 10);
	penstroke z5e--z6e;

	% bottom horizontal
	rt bot z8 = (w-sx#, 0);
	penpos8(mg_t, 40);
	bot z4 = (0.5w, 0);
	penpos4(mg_t, 40);
	penstroke z4e--z8e;

	penlabels(1,2,3,4,22,5,6,8);
endchar;

beginjur("2", num_width);
	penmonglyph(mg_t) ;
	z1 = (0.5w,0.4hh#);
	lft z2 = (sx#+0.1w, 0.75hh#);
	top z3 = (0.5w, hh#);
	rt z4 = (0.8w-sx#, 0.5hh#);
	bot lft z5 = (sx#, 0);
	bot rt z6 = (w-sx#, 0);
	rt z7 = (w-sx#, 0.3hh#);
	draw z1...z2...z3...z4{z1-z4}..{down}z5--z6--z7;
	%penstroke z1e..z2e{up}..z3e{right}..z4e{z1-z4}..{down}z5e--z6e--z7e;
	penlabels(1,2,3,4,5,6,7);
endchar;


beginjur("3", num_width);
	penmonglyph(mg_t) ;
	top lft z11 = (sx#, hh#);
	lft bot z12 = (sx#, 0);
	penpos11(mg_t, 15);
	penpos12(mg_t, 15);
	penstroke z11e--z12e;

	top lft z21 = (sx#, hh#);
	top z22 = (w-sx#, hh#);
	penpos21(mg_t, 20);
	penpos22(mg_t, 20);
	penstroke z21e--z22e;

	top z31 = (0.5w, hh#);
	top z32 = (0.5w, 0.6hh#);
	z33 = (w-sx#, 0.3hh#);
	bot z34 = (0.5w, 0);
	bot z35 = (0.35w, y32/5);
	penpos31(mg_t, 15);
	penpos32(mg_t, 15);
	penpos33(mg_t, 15);
	penpos34(mg_t, -180+30);
	penpos35(mg_t, -185+20);
	%penstroke z31e--z32e{right}..{down}z33e
	penstroke z31e--z32e{dir -10}..z33e
	         % {down}
		  ..{left}z34e..{up}z35e;
	penlabels(11,12,21,22,31,32,33,34,35);
endchar;

beginjur("4", num_width);
	penmonglyph(mg_t) ;
	top z11 = (sx#, hh#);
	top z12 = (w-sx#, hh#);
	penpos11(mg_t, 20);
	penpos12(mg_t, 20);
	penstroke z11e--z12e;
	picture p;
	p := currentpicture shifted (0, -0.3hh#);
	addto currentpicture also p;
	x31 = x32;
	.66(w - 2sx#)  + sx# = x31;
	y31 = y11;
	y32 = 0;
	penpos31(mg_t, 20);
	penpos32(mg_t, 20);
	penstroke z31e--z32e;
	penlabels(11,12,31,32);
endchar;

beginjur("5", num_width);
	penmonglyph(mg_t) ;
	top y11 = hh#;
	y11 = y12 = y31;
	lft x11 = lft x21 = sx#;
	y21 = y22 = .5hh#;
	rt x12 = w - sx#;
	x22 = x31 = x23 =  0.4(x11 + x12);
	penpos11(mg_t,50);
	penpos12(mg_t,30);
	penstroke z11e--z12e;
	penpos21(mg_t, 50);
	penpos22(mg_t, 20);
	penpos225(mg_t, 20);
	penpos23(mg_t, -180+20);
	bot y23 = 0;
	z225 = (x12, (y22+y23)/2);
	penpos31(mg_t, -180-30);
	penstroke z11e--z21e--z22e{right}..{down}z225e{down}..{left}z23e--z31e;
	penlabels(11,12,21,22,225,23,31);
endchar;

beginjur("6", num_width);
	penmonglyph(mg_t) ;
	lft x11 = lft x22 = lft x23 = sx#/2;
	rt x12 = w - sx#/2;
	top y11 = top y12 = hh#;  
	penpos11(mg_t, 40);
	penpos12(mg_t, 40);
	penstroke z11e--z12e;
	x31 = x32 = (x11+x12)/2;
	y31 = y11;
	bot y22 = bot y32 = 0;
	y23 = .4(y11+y22);
	x225 = .45(x11+x31);
	y225 = .2(y22+y23);
	penpos22(mg_t, 40);
	z91l = z22l;
	z91r  = z22r rotatedaround(z22l, 50);
	z91 = .5[z91r, z91l];
	penpos225(mg_t, 180);
	penpos23(mg_t, 270);
	penpos31(mg_t, 40);
	penpos32(mg_t, 40);
	penstroke z11e--z22e;
	penstroke z91e{right}...{up}z225e{up}...{left}z23e;
	penlabels(11,12,22,23,31,32);
	penstroke z31e--z32e;
	z41 = (x12, y32);
	penpos41(mg_t, 40);
	penstroke z12e--z41e;
endchar;

beginjur("7", num_width);
	penmonglyph(mg_t);
	lft x1 = lft x4 = sx#;
	x2 = x5 = (x1+x3)/2;
	rt x3 = rt x6 = w - sx#;
	x7 = (x2+x3)/2;
	top y1 = top y2 = top y3 = hh#;
	y4 = y5 = y6 = (y1+y7)/2;
	y7 = 0;
	penpos1(mg_t,20);
	penpos2(mg_t,20);
	penpos3(mg_t,20);
	penpos4(mg_t,20);
	penpos5(mg_t,20);
	penpos6(mg_t,20);
	penpos7(mg_t,20);
	penstroke z1e--z3e;
	penstroke z2e--z5e;
	penstroke z4e--z6e--z7e;
endchar;

beginjur("8", num_width);
	penmonglyph(mg_t);
	rt x2 = rt x8 = w - sx#;
	top y2 = hh#;
	bot y8 = 0;
	penpos2(mg_t, 20);
	penpos8(mg_t, 20);
	rt x6 = x2;
	y6 = (y2+y8)/2;
	z4 = (.7(x5+x6), y6);
	y5 = y4;
	x5 = sx#;
	x7 = (x5 + x4) / 2;
	y7 = y8;
	x3 = x7;
	x1 = (x3 + x4) / 2;
	y3 = 2(y5 + y4) / 3;
	y1 = (y2+y6)/2;
	z11 = z1;
	z44 = z6;
	penpos1(mg_t, -100);
	penpos3(mg_t, 10);
	penpos4(mg_t, 80);
	penpos11(mg_t, 270);
	penstroke z2e--z8e;
	penstroke z1e{left}..z3e{down}..z4e{right}..{left}cycle;
	%penpos44(mg_t, 40);
	%penpos5(mg_t, 80);
	%penpos7(mg_t, 170);
	penpos44(mg_t, 40);
	penpos5(mg_t, 40);
	z55l = z5l;
	z55r = z5r rotatedaround(z5l, 140);
	z55 = .5[z5l, z5r];
	penpos7(mg_t, 270);
	penpos6(mg_t, 270);
	penstroke z44e--z5e--z55e...{(1,-1)}z7e{(1,1)}...z6e;

endchar;

beginjur("9", num_width);
	penmonglyph(mg_t);
	lft x11 = lft x22 = sx#/2;
	rt x12 =  rt x91 = rt x23 = w - sx#/2;
	top y11 = top y12 = hh#;  
	penpos11(mg_t, 40);
	penpos12(mg_t, 40);
	penstroke z11e--z12e;
	x31 = x32 = (x11+x12)/2;
	y31 = y11;
	bot y22 = bot y32 = 0;
	y23 = .4(y11+y22);
	x225 = .43(x12+x31);
	y225 = .2(y22+y23);
	penpos22(mg_t, 40);
	y91 = y22;
	penpos91(mg_t, 40);
	penpos225(mg_t, 40);
	penpos23(mg_t, -50);
	penpos31(mg_t, 40);
	penpos32(mg_t, 40);
	penstroke z11e--z22e;
	penstroke z91e{left}...{up}z225e{up}...{right}z23e;
	penlabels(11,12,22,23,31,32);
	penstroke z31e--z32e;
	z41 = (x12, y32);
	penpos41(mg_t, 40);
	penstroke z12e--z41e;
endchar;

% BEGIN_UPPER
%
def GLYPH_UPPER_A(expr ch) =
beginjur(ch, 6u#);
	penmonglyph(mg_t) ;
	z11 = (sx#, aheight * hh#);
	z12 = (w-sx#, aheight * hh#);
	penpos11(0.4mg_t, 90);
	penpos12(0.4mg_t, 90);
	penstroke z11e--z12e;

	top lft z21 = (sx#, hh#);
	bot z22 = (0.5w, 0);
	top rt z23 = (w-sx#, hh#);
	draw z21--z22--z23;
	penlabels(11,12,21,22,23);
endchar
enddef;
GLYPH_UPPER_A("A");

def GLYPH_UPPER_B(expr ch) =
beginjur(ch, 8u#);
	penmonglyph(mg_t) ;
	top z11 = (sx#, hh#);
	top z12 = (w-sx#, hh#);
	penpos11(mg_t, 20);
	penpos12(mg_t, 20);
	penstroke z11e--z12e;

	top z21 = (sx#+0.1w, 0.5[hh#,y11]);
	z22 = (x21, 0);
	penpos21(mg_t, 20);
	penpos22(mg_t, 20);
	penstroke z21e--z22e;

	z31 = (x21, 0.4h);
	z32 = (0.4w, 0.3h);
	z33 = lft z22;
	draw z31{right}..{down}z32{down}..{left}z33;

	rt z41 = (0.9w-sx#, y21);
	bot rt z42 = (0.9w-sx#, 0);
	penpos41(mg_t, 20);
	penpos42(mg_t, 20);
	penstroke z41e--z42e;
	penlabels(11,12,21,22,31,32,33,41,42);
endchar
enddef;
GLYPH_UPPER_B("B");


def GLYPH_UPPER_C(expr ch) =
beginjur(ch, 6u#);
	penmonglyph(mg_t) ;
	z11 = (0.4w, hh#);
	z12 = (0.4w, 0);
	penpos11(mg_t, 20);
	penpos12(mg_t, 20);
	penstroke z11e--z12e;

	lft z21 = (sx#, 0.8hh#);
	rt z22 = (w-sx#, 0.5hh#);
	lft z23 = (sx#, 0.2hh#);
	penpos21(mg_t, 40);
	penpos22(mg_t, 0);
	penpos23(mg_t, 120+180);
	penstroke reverse z21e{3,1}..{down}z22e;
	penstroke  z22e{down}..{-3,1}z23e;
	penlabels(11,12,21,22,23);
endchar
enddef;
GLYPH_UPPER_C("C");

def GLYPH_UPPER_D(expr ch) =
beginjur(ch, 6u#);
	penmonglyph(mg_t) ;
	z11 = (0.6w, hh#);
	z12 = (0.6w, 0);
	penpos11(mg_t, 20);
	penpos12(mg_t, 20);
	penstroke z11e--z12e;

	rt z21 = (w-sx#, 0.8hh#);
	lft z22 = (sx#, 0.5hh#);
	rt z23 = (w-sx#, 0.2hh#);
	penpos21(mg_t, -40);
	penpos22(mg_t, 0);
	penpos23(mg_t, 50);
	penstroke reverse z22e{up}..{3, -1}z21e;
	penstroke  reverse z22e{down}..{3,1}z23e;
	penlabels(11,12,21,22,23);
endchar
enddef;
GLYPH_UPPER_D("D");

def GLYPH_UPPER_E(expr ch) =
beginchar(ch, 6u#, 10pt#, 3pt#);
	penmonglyph(mg_t) ;
	top lft z1 = (sx#, hh#);
	z2 = 0.8[z1, z3];
	rt z3 = (w-sx#, hh#);
	lft z4 = (sx#, .4hh#);
	draw z4--z1--z3;

	
	%bot z6 = (x2, -.5d);
	bot z6 = (x2, 0);
	lft bot z5 = (sx#, -.5d);
	%draw z2..controls z6..z5;
	draw reverse z2..controls z6..z5;

endchar
enddef;
GLYPH_UPPER_E("E");

def GLYPH_UPPER_F(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t) ;
	top lft z1 = (sx#, hh#);
	top rt z2 = (w - 2sx#, hh#);
	z3 = (x1, .5hh#);
	z4 = .5[z3, z5];
	rt z5 = (w - sx#-mg_t, y3);
	bot z6 = (x1, 0);
	z7 = (x4, y6);
	z8 = (x5, y6);
	draw z2--z1--z6;
	draw z3--z5{curl 0.1}..z8..controls z7..{up}z4;
endchar
enddef;
GLYPH_UPPER_F("F");

def GLYPH_UPPER_G(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t) ;
	top z1 = (.5w, hh#);
	z2 = z1 rotatedaround((.5w,.5hh#), -55);
	lft z3 = (sx#, .5hh#);
	bot z6 = (x1, 0);
	z4 = (.6w, .4hh#);
	z7 = (x4, .5(0+y4));
	z8 = (x4, aheight * hh#);
	rt z5 = (w - sx#, .25hh#);
	draw z2..z1..z3..z6..{up}z5..{left}z4--z7;
	draw z8--z7;
endchar
enddef;
GLYPH_UPPER_G("G");



def GLYPH_UPPER_H(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t) ;
	top lft z1 = (sx#+.2w, hh#);
	top z2 = (.5w, hh#);
	top rt z3 = (w-sx#, hh#);
	lft z4 = (sx#, .6hh#);
	z5 = (.5w, .5hh#);
	z6 = (w-sx#, .6hh#);
	bot z7 = (x1, 0);

	draw z4..z5..controls z6 and z3..z2..controls z1..z7;

	z8 = (x1, .3hh#);
	rt z9 = (w - sx#, y8);
	bot z10 = (x9, 0);
	draw z8--z9--z10;

endchar
enddef;
GLYPH_UPPER_H("H");


def GLYPH_UPPER_I(expr ch) =
beginjur(ch, 3u#);
	penmonglyph(mg_t);
	numeric dx, dy, xL, xR;
	dx = .1;
	dy = .1;
	%xL = (0.5-dx) * w;
	%xR = (0.5+dx) * w;
	xL = sx#;
	xR = w - sx#;
	top z1 = (xL, hh#);
	top z2 = (xR, (1-dy) * hh#);
	bot z3 = (xL, dy * hh#);
	bot z4 = (xR, 0);
	draw z1--z2--z3--z4;
	z5 = (xL, (0.5+dy) * hh#);
	z6 = (xR, (0.5-dy) * hh#);
	draw z5--z6;
endchar
enddef;
GLYPH_UPPER_I("I");

def GLYPH_UPPER_J(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	lft top z1 = (sx# + 0.1(w-2sx#), hh#);
	rt top z2 = (sx# + 0.8(w-2sx#), hh#);
	top z3 = (sx# + 0.6(w-2sx#), y1);
	z4 = (x3, 0.9hh#);
	z5 = (x3, 0.2hh#);
	z6 = (x3, 0);
	z7 = (sx#, 0.25hh#);
	z8 = (0.5w, 0.5hh#);
	z9 = (w-sx#, 0.5hh#);
	draw z1--z2..controls z3..z4--z5..controls z6..z7..{right}z8--z9;
endchar
enddef;
GLYPH_UPPER_J("J");

def GLYPH_UPPER_K(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	top lft z1 = (sx#, hh#);
	top rt z2 = (w-sx#, hh#);
	bot lft z3 = (sx#, 0);
	bot rt z4 = (w-sx#, .4hh#);
	bot z5 = (.5w, .5(0+y4));
	bot rt z6 = (w-sx#, 0);

	draw z3--z1--z2--z6{left}..z5..{right}z4;

endchar
enddef;
GLYPH_UPPER_K("K");

def GLYPH_UPPER_L(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	top z1 = (0.5w, hh#);
	top rt z2 = (w-sx#, hh#);
	bot lft z3 = (0, 0);
	bot z4 = (0.45w, .05hh#);
	bot z8 = (0.6(x4+x6), .04hh#);
	top rt z6 = (w-sx#, hh#);
	bot rt z7 = (w-sx#, 0);
	draw reverse z3{right}..controls z4..{up}z1;
	draw z3--z4{right}..controls z8..{up}z6;
	draw z6{down}..controls z8..{right}z7;
endchar
enddef;
GLYPH_UPPER_L("L");

def GLYPH_UPPER_M(expr ch) =
beginjur(ch, 9u#);
	penmonglyph(mg_t);
	top y1 = top y3 = top y5 = hh#;
	bot y2 = bot y4 = 0;
	x2-x1 = 1.3(x3-x2) = 1.3(x4-x3) = x5-x4 = (w-2sx#-2mg_t)/4;
	x3 = .5w;
	draw z1--z2--z3--z4--z5;
	y6 = y7 = y8 = aheight * hh#;
	x6 = sx#;
	rt x7 = w - sx#;
	x8 = .5w;
	draw z6 ..controls z8.. z7;
endchar
enddef;
GLYPH_UPPER_M("M");

def GLYPH_UPPER_N(expr ch) =
beginjur(ch, 6u#);
	penmonglyph(mg_t);
	top lft z1 = (sx#, hh#);
	bot lft z2 = (sx#, 0);
	top lft z3 = (sx#, 0.9hh#);
	top rt z4 = (w-sx#, hh#);
	bot rt z5 = (w-sx#, 0);
	bot z6 = (0.6w, 0.02h);
	draw z1--z2..controls z3..z4--z5--z6;
endchar
enddef;
GLYPH_UPPER_N("N");

def GLYPH_UPPER_O(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	x5 = sx#;
	x6 = w - sx#;
	y5 = y6 = .5hh#;
	draw z5--z6;
	x1 = x3 = .5w;
	top y1 = hh#;
	bot y3 = 0;
	lft x2 = sx# + 0.1(x6-x5);
	rt x4 = sx# + 0.9(x6-x5);
	y2 = y4 = .5hh#;
	draw z1..z2..z3..z4..cycle;
endchar
enddef;
GLYPH_UPPER_O("O");

def GLYPH_UPPER_P(expr ch) =
beginjur(ch, 8u#);
	penmonglyph(mg_t);
	lft x1 = lft x2 = sx#;
	x3 = lft x2 + .5mg_t;
	top y1 = hh#;
	bot y2 = 0;
	draw z1--z2;
	bot y3 = bot y4 = bot y5 = .1hh#;
	x4 = x3 +.1w;
	x5 = x6 = w - sx#;
	x7 = .5w;
	y6 = y7 = aheight * hh#;
	draw z3..{right}z4{right}..controls z5 and z6..z7;
endchar
enddef;
GLYPH_UPPER_P("P");

def GLYPH_UPPER_Q(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	x1 = x3 = .5w;
	top y1 = hh#;
	bot y3 = 0;
	lft x2 = sx#;
	rt x4 = w - sx#;
	y2 = y4 = .5hh#;
	draw z1..z2..z3..z4..cycle;
	rt x7 = rt x8 = w - sx#;
	top y7 = hh#;
	bot y8 = 0;
	draw z7--z8;
endchar
enddef;
GLYPH_UPPER_Q("Q");

def GLYPH_UPPER_R(expr ch) =
beginjur(ch, 7u#);
	penmonglyph(mg_t);
	lft x1 = lft x2 = sx#+0.1(w-2sx#);
	top y1 = hh#;
	bot y2 = 0;
	draw z1--z2;
	z3 = (.5w, .9hh#);
	z35 = (sx#+0.8(w-2sx#), .9hh#);
	z4 = (sx#+0.8(w-2sx#), .5hh#);
	z5 = (.6(x6+x4), .1hh#);
	z55 = 0.5[z5, z6];
	z6 = (x1, 0.88y4);
	z7 = (sx#, y4);
	z8 = (w-sx#, y4);
	draw z3{right}..controls z35..z4{down}..{left}z5{left}
		..z55..z6;
	draw z7--z8;
endchar
enddef;
GLYPH_UPPER_R("R");

def GLYPH_UPPER_S(expr ch) =
beginjur(ch, 8u#);
	penmonglyph(mg_t);
	lft x1 = sx#;
	lft x2 = w - sx#;
	y1 = y2 = aheight * hh#;
	draw z1--z2;
	z4 = 0.67[z1,z2];
	z3 = (.5(x4+x2), hh#);
	bot z5 = (.5(x1+x4), 0);
	x6 = x7 = x1+0.2(x2-x1);
	y6 = .2hh#;
	y7 = hh#;
	draw z2{up}..{left}z3..z4..{left}z5..{up}z6--z7;
endchar
enddef;
GLYPH_UPPER_S("S");

def GLYPH_UPPER_T(expr ch) =
beginjur(ch, 8u#);
	penmonglyph(mg_t);
	lft x1 = lft x2 = sx#;
